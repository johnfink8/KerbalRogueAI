        <and>
            <LandedAt>Runway</LandedAt>
            <EngineType>turbine</EngineType>
            <DeltaV>13000</DeltaV>
            <MassGreater>126.1</MassGreater>
            <ModuleType>ModuleLiftingSurface</ModuleType>
        </and>
    </StartConditions>
    <Maneuvers>
