<FlightPlan>
    <StartConditions>
        <and>
            <OrbitCircular>Kerbin</OrbitCircular>
            <DeltaV>5000</DeltaV>
            <BodyExists>Moho</BodyExists>
            <TransferWindow>Moho</TransferWindow>
            <RandomChance>50</RandomChance>
        </and>
    </StartConditions>
    <Maneuvers>
        <PlanetTransfer>Moho</PlanetTransfer>
        <CourseCorrection target="Moho" distance="200"/>
        <WarpTo>Transition</WarpTo>
        <ChangePE>30000</ChangePE>
        <Circularize>PE</Circularize>
    </Maneuvers>
</FlightPlan>
